{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "Portfolio microapp",
  "repository": "https://github.com/rfprod/portfolio.git",
  "license": "MIT",
  "author": "rfprod",
  "scripts": {
    "preinstall": "rm -rf node_modules/",
    "postinstall": "npx sort-package-json && npm run ngcc",
    "lint": "npx eslint -c .eslintrc.json *.js functions/**/*.js test/**/*.js && npm run ng-lint",
    "ng": "ng",
    "ng-build": "ng build",
    "ng-build-prod": "ng build --prod",
    "ng-serve-dev": "ng serve --proxy-config proxy.conf.json",
    "ng-serve-prod": "ng serve --prod --proxy-config proxy.conf.prod.json",
    "ng-test": "ng test",
    "ngcc": "ngcc --properties es2015",
    "prettier:html": "npx prettier -c src/**/*.html",
    "prettier:html:fix": "npx prettier -c --write src/**/*.html",
    "preprotractor": "npm run update-webdriver",
    "protractor": "node server.js & protractor test/protractor.conf.js",
    "start": "node server.js & npm run ng-serve-dev",
    "stylelint": "npx stylelint --syntax scss",
    "pretest": "npm install",
    "test": "npm run ng-test",
    "test-single-run": "ng test portfolio --watch=false",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/animations": "9.1.2",
    "@angular/cdk": "9.2.1",
    "@angular/common": "9.1.2",
    "@angular/compiler": "9.1.2",
    "@angular/core": "9.1.2",
    "@angular/flex-layout": "9.0.0-beta.29",
    "@angular/forms": "9.1.2",
    "@angular/material": "9.2.1",
    "@angular/material-moment-adapter": "9.2.1",
    "@angular/platform-browser": "9.1.2",
    "@angular/platform-browser-dynamic": "9.1.2",
    "@angular/router": "9.1.2",
    "@ngneat/until-destroy": "7.1.6",
    "@types/core-js": "2.5.3",
    "@types/hammerjs": "2.0.36",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "13.13.0",
    "core-js": "3.6.5",
    "firebase": "7.14.1",
    "firebase-tools": "8.1.1",
    "hammerjs": "2.0.8",
    "material-design-icon-fonts": "3.0.1",
    "moment": "2.24.0",
    "reflect-metadata": "0.1.13",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.5.5",
    "typescript": "3.8.3",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.1",
    "@angular/cli": "9.1.1",
    "@angular/compiler-cli": "9.1.2",
    "@angular/language-service": "9.1.2",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "13.13.0",
    "body-parser": "1.19.0",
    "codelyzer": "5.2.2",
    "compression": "1.7.4",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "express": "4.17.1",
    "http-server": "0.12.1",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "5.0.1",
    "karma": "5.0.2",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.3",
    "prettier": "2.0.4",
    "prettier-stylelint": "0.4.2",
    "protractor": "5.4.4",
    "sort-json": "^2.0.0",
    "sort-package-json": "1.42.0",
    "stylelint": "13.3.2",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-prettier": "1.1.2",
    "ts-node": "8.8.2",
    "tslint": "6.1.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-etc": "1.10.1",
    "tslint-plugin-prettier": "2.3.0"
  },
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.14.4"
  }
