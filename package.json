    "jest-preset-angular": "8.2.1",
    "prettier": "2.0.5",
    "prettier-stylelint": "0.4.2",
    "sort-json": "2.0.0",
    "sort-package-json": "1.44.0",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-prettier": "1.1.2",
    "ts-jest": "26.2.0",
    "ts-loader": "8.0.2",
    "ts-node": "9.0.0",
    "webpack-bundle-analyzer": "3.8.0"
  },
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.14.4"
  }
}
