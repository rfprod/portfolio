{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "Portfolio microapp",
  "repository": "https://github.com/rfprod/portfolio",
  "license": "MIT",
  "author": "rfprod",
  "scripts": {
    "analyze:app": "npm run ng:build:app:stats; npx webpack-bundle-analyzer ./dist/stats-es2015.json",
    "postinstall": "npx sort-package-json && npm run ngcc",
    "ng": "ng",
    "ng:build": "ng build",
    "ng:build:app:stats": "ng build --stats-json --configuration analyze-bundle",
    "ng:build:prod": "ng build --prod",
    "ng:e2e": "ng e2e",
    "ng:e2e:headless": "ng e2e --headless",
    "ng:e2e:headless:single-run": "ng e2e --headless --watch=false",
    "ng:lint": "ng lint",
    "ng:serve:dev": "ng serve --proxy-config proxy.conf.json",
    "ng:serve:prod": "ng serve --prod --proxy-config proxy.conf.prod.json",
    "ng:test": "ng test",
    "ng:test:single-run": "ng test --watch=false",
    "ngcc": "ngcc --tsconfig tsconfig.base.json",
    "precommit:lint": "ng lint; yarn prettier:html; yarn stylelint",
    "prettier:html": "npx prettier -c src/**/*.html",
    "prettier:html:fix": "npx prettier -c --write src/**/*.html",
    "start": "node server.js & npm run ng:serve:dev",
    "stylelint": "npx stylelint \"./src/**/*.scss\"",
    "stylelint:fix": "npx stylelint \"./src/**/*.scss\" --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit:lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@angular/animations": "10.0.11",
    "@angular/cdk": "10.1.3",
    "@angular/common": "10.0.11",
    "@angular/compiler": "10.0.11",
    "@angular/core": "10.0.11",
    "@angular/flex-layout": "10.0.0-beta.32",
    "@angular/forms": "10.0.11",
    "@angular/material": "10.1.3",
    "@angular/material-moment-adapter": "10.1.3",
    "@angular/platform-browser": "10.0.11",
    "@angular/platform-browser-dynamic": "10.0.11",
    "@angular/router": "10.0.11",
    "@briebug/cypress-schematic": "3.3.0",
    "@ngneat/until-destroy": "8.0.1",
    "@ngxs/form-plugin": "3.6.2",
    "@ngxs/router-plugin": "3.6.2",
    "@ngxs/store": "3.6.2",
    "@types/core-js": "2.5.3",
    "@types/hammerjs": "2.0.36",
    "@types/node": "14.6.0",
    "core-js": "3.6.5",
    "firebase": "7.19.0",
    "firebase-tools": "8.9.0",
    "hammerjs": "2.0.8",
    "material-design-icon-fonts": "3.0.1",
    "moment": "2.27.0",
    "reflect-metadata": "0.1.13",
    "roboto-fontface": "0.10.0",
    "rxjs": "6.6.2",
    "typescript": "3.9.7",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "10.0.0",
    "@angular-devkit/build-angular": "0.1000.7",
    "@angular-eslint/builder": "0.1.0-beta.1",
    "@angular-eslint/eslint-plugin": "0.1.0-beta.1",
    "@angular-eslint/eslint-plugin-template": "0.1.0-beta.1",
    "@angular-eslint/template-parser": "0.1.0-beta.1",
    "@angular/cli": "10.0.7",
    "@angular/compiler-cli": "10.0.11",
    "@angular/language-service": "10.0.11",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@cypress/webpack-preprocessor": "5.4.4",
    "@ngxs/logger-plugin": "3.6.2",
    "@types/jest": "26.0.10",
    "@types/jsdom": "16.2.3",
    "@types/node": "14.6.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "body-parser": "1.19.0",
    "codelyzer": "6.0.0",
    "compression": "1.7.4",
    "cypress": "5.0.0",
    "dotenv": "8.2.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-rxjs": "0.0.2-beta.28",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-watch": "7.0.0",
    "express": "4.17.1",
    "http-server": "0.12.3",
